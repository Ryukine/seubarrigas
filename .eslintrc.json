{
    "env": {
        "browser": true,
        "es2021": true,
        "jest": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "linebreak-style": "off",
        "arrow-body-style": "off",
        "max-len": "off",
        "consistent-return": "off",
        "no-multiple-empty-lines": "off"
    }
}